{
  "name": "efonelfo",
  "version": "0.0.6",
  "license": "MIT",
  "repository": "skalar/efonelfo",
  "main": "lib/index.js",
  "files": [
    "README.md",
    "lib/*"
  ],
  "engines": {
    "node": ">= 7.10.0"
  },
  "scripts": {
    "build": "yarn run clean && babel src --minified --compact true --source-maps false -d lib",
    "clean": "rm -fr lib coverage .nyc_output",
    "lint": "eslint src",
    "test": "NODE_ENV=test babel-tape-runner test/*.test.js | tap-spec",
    "watch": "nodemon -C -w src -w test -x yarn run test",
    "coverage": "yarn run clean && nyc yarn test && nyc report --reporter=html",
    "prepublish": "yarn run build"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-export-extensions"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "exclude": [
            "transform-async-to-generator"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul",
          "transform-object-rest-spread"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "skalar"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-tape-runner": "^2.0.1",
    "eslint": "^3.19.0",
    "eslint-config-skalar": "^0.0.7",
    "istanbul": "^0.4.5",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "csv-parse": "^1.2.0",
    "iconv-lite": "^0.4.18"
  }
}
